    # Thalamus (M)    
    subsection constitutive@[type=modified_one_term_ogden_atrophy,instance=1,material_id=10]
          set alpha = -15.15002338
          set poisson ratio = 0.49
          set empirical coefficient = -2.
          set mu = 137.6724163e-6
          set normal atrophy rate = 0.0015
