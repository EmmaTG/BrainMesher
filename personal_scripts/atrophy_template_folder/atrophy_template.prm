subsection experiment
  set reset = true

  subsection testing_device@[type=atrophy,instance=1]
    set input files = %testing_device%
	set max radius = %radius%
	set center = %center%
  end

  subsection sample

    subsection finite element
      set polynomial degree = 1
    end

    subsection geometry@[type=import]
      set inpFile = %inp_file%
    end  
    
     # Amygdala (A)   
    subsection constitutive@[type=modified_one_term_ogden_atrophy,instance=1,material_id=18]
          set alpha = -14.16400654
          set poisson ratio = 0.49
          set empirical coefficient = -2.
          set mu = 193.3378573e-6
          set normal atrophy rate = 0.0015
          set enhanced atrophy rate = 0.0035
    end
    
     # Basal Ganglia (BG)   
    subsection constitutive@[type=modified_one_term_ogden_atrophy,instance=1,material_id=11]
          set alpha = -15.04778681
          set poisson ratio = 0.49
          set empirical coefficient = -2.
          set mu = 169.6653712e-6
          set normal atrophy rate = 0.0015
          set enhanced atrophy rate = 0.0035
    end 
    
    # Brain stem (BS)
