subsection experiment
  set reset = true

  subsection testing_device@[type=atrophy,instance=1]
    set input files = %testing_device%
	set max radius = %radius%
	set center = %center%
  end

  subsection sample

    subsection finite element
      set polynomial degree = 1
    end

    subsection geometry@[type=import]
      set inpFile = %inp_file%
    end  

	# AMYGDALA (Am)
	subsection constitutive@[type=modified_one_term_ogden,instance=1,material_id=18]
		 set empirical coefficient = -2
		 set alpha = -19.66623566
		 set mu = 257.0497315
		 set poisson ratio = 0.45
	end

	# CORTEX (C)
	subsection constitutive@[type=modified_one_term_ogden,instance=1,material_id=3]
		 set empirical coefficient = -2
		 set alpha = -19.27409673
		 set mu = 281.7518902
		 set poisson ratio = 0.45
	end

	# BASALGANGLIA (BG)
	subsection constitutive@[type=modified_one_term_ogden,instance=1,material_id=26]
		 set empirical coefficient = -2
		 set alpha = -18.94830956
		 set mu = 252.1460859
		 set poisson ratio = 0.45
	end

	# BRAINSTEM (BS)
	subsection constitutive@[type=modified_one_term_ogden,instance=1,material_id=16]
		 set empirical coefficient = -2
		 set alpha = -20.4129503
		 set mu = 233.1224979
		 set poisson ratio = 0.45
	end

	# CORONARADIATA (CR)
	subsection constitutive@[type=modified_one_term_ogden,instance=1,material_id=2]
		 set empirical coefficient = -2
		 set alpha = -19.70393753
		 set mu = 157.0888632
		 set poisson ratio = 0.45
	end

	# CORPUSCALLOSUM (CC)
	subsection constitutive@[type=modified_one_term_ogden,instance=1,material_id=251]
		 set empirical coefficient = -2
		 set alpha = -14.93641813
		 set mu = 61.66031083
		 set poisson ratio = 0.45
	end

	# CEREBELLUM (CB)
	subsection constitutive@[type=modified_one_term_ogden,instance=1,material_id=7]
		 set empirical coefficient = -2
		 set alpha = -23.22191238
		 set mu = 177.4922384
		 set poisson ratio = 0.45
	end

	# MIDBRAIN (M)
	subsection constitutive@[type=modified_one_term_ogden,instance=1,material_id=10]
		 set empirical coefficient = -2
		 set alpha = -18.68414503
		 set mu = 215.6963735
		 set poisson ratio = 0.45
	end

	# HIPPOCAMPUS (Hi)
	subsection constitutive@[type=modified_one_term_ogden,instance=1,material_id=17]
		 set empirical coefficient = -2
		 set alpha = -17.36572538
		 set mu = 184.2088107
		 set poisson ratio = 0.45
	end


    # CSF (~)
    subsection constitutive@[type=modified_one_term_ogden,instance=1,material_id=24]
          set alpha = -1.0
          set poisson ratio = 0.3
          set empirical coefficient = -2.
          set mu = 10.e-6
    end 
      
    subsection nonlinear_solver_control
      set absolute tolerance = 1e-9
      set linear solver type = CG
      set maximum number of iterations = 10
      set relative tolerance = 1e-06
    end

    subsection quadrature
      set quadrature points per space direction = auto
      set quadrature type = gauss
    end

  end
end

subsection global
  set output directory = /calculate/efiSim1F/build_atrophy/out/%output%

  set input directory  = /calculate/efiSim1F/build_atrophy/in
  
  set output filename = %output%
  
  set paraview output = true
  
  set move mesh = false

  # options: quiet|normal|verbose|very verbose|debug
  set verbosity console = debug

  # options: quiet|normal|verbose|very verbose|debug
  set verbosity logfile = normal
end
